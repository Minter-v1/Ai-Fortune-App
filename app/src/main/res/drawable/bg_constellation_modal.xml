<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 배경색 (66% 투명한 흰색) -->
    <solid android:color="#66FFFFFF" />

    <!-- 모서리 둥글게 (20dp 반지름) -->
    <corners android:radius="20dp" />

    <!-- 테두리 (2dp 흰색) -->
    <stroke
        android:width="2dp"
        android:color="#FFFFFF" />

</shape>